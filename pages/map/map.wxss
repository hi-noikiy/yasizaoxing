/* map.wxss */
page {
  height: 100%
}
.map-container {
  width: 100%;
  height: 100%;
}
.map-container map {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  width: 100%;
  height: 100%;
}
.top {
  width: 100%;
  font-size: 14px;
  color: #f89405;
  line-height: 33px;
  padding: 0 10px;
  padding-left: 40px;
  background: #fffcee url(data:image/png;base64,) no-repeat 10px center;
  background-size: 20px 20px;
}
.adv {
  padding-left: 10px;
  padding-right: 10px;
  max-height: 50px;
  margin-top: 10px;
  box-sizing: border-box;
}
.btn {
  position: fixed;
  bottom: 5%;
  width: 40%;
  line-height: 40px;
  background-color: #6cc0f0;
  color: #fff;
  left: 50%;
  margin-left: -20%;
  font-size: 16px;
  border-radius: 30px;
  text-align: center;
  box-shadow: 0 0 10px #6cc0f0;
}
.btn.a_login {
  background-color: #382e2f;
  box-shadow: 0 0 10px #382e2f;
}
.btn.a_login image {
  margin-left: 5px;
}
.shopinfo {
  position: absolute;
  width: 100%;
  height: 93px;
  background-color: #fff;
}
.shopinfo .text {
  padding-left: 77px;
  line-height: 22px;
}
.shopinfo .text .title {
  font-size: 15px;
  color: #565656;
}
.shopinfo .text .sub-title {
  font-size: 14px;
  color: #828282;
}
.shop-img {
  position: absolute;
  left: 10px;
  top: 50%;
  margin-top: -28px;
  width: 57px;
  height: 57px;
  border-radius: 50%;
  background-color: #d0d5d2;
  text-align: center;
}
.shop-img image {
  position: absolute;
  top: 50%;
  margin-top: -30%;
  left: 50%;
  margin-left: -30%;
  width: 60%;
  height: 60%;
}
.to_right {
  position: absolute;
  right: 10px;
  top: 50%;
  width: 10px;
  margin-top: -9px;
}
.wallet {
  position: absolute;
  width: 30px;
  height: 30px;
  bottom: 5%;
  right: 10%;
  border-radius: 50%;
  background-color: #fff;
  box-shadow: 2px 2px 10px #999;
}
.wallet image {
  width: 60%;
  height: 60%;
}
.wallet.order {
  bottom: 14%
}
.mask {
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  background-color: transparent;
}