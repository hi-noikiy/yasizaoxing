/* order.wxss */
page {
  height: 100%;
  background-color: #f3f3f3;
  overflow: hidden
}
.scrollview {
  height: 100%;
}
.head {
  position: absolute;
  width: 50px;
  height: 50px;
  left: 15px;
  top: 50%;
  margin-top: -25px;
  border-radius: 3px
}
.title {
  margin-bottom: 14px;
  padding: 0 10px;
  padding-top: 25px;
  color: #333333;
  font-size: 17px;
}
.list {
  padding: 0 10px;
}
.list .li {
  position: relative;
  padding: 15px 8px;
  padding-left: 80px;
  margin: 15px 0;
  background-color: #fff;
  box-shadow: 2px 2px 10px #e8e8e8
}
.list .li .i {
  overflow: hidden;
  padding-left: 18px;
  background-repeat: no-repeat;
  background-position: 0 center;
}
.list .li .top {
  margin-bottom: 10px;
  font-size: 12px;
  color: #333;
  background-image: url(data:image/png;base64,);
  background-size: 10px 10px;
}
.list .li .price {
  color: #e3005a
}
.list .li text:first-child {
  width: 60%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.list .li text.status {
  position: absolute;
  top: 50%;
  right: 10px;
  line-height: 0;
  color: #2f9edd;
  font-size: 15px;
}
.list .li .bottom {
  font-size: 12px;
  color: #999999;
  background-image: url(data:image/png;base64,);
  background-size: 12px;
}