/* card.wxss */
page, .card-container {
  height: 100%;
}
.card-container {
  background-color: #f0eff5;
}
.top {
  padding: 30px 15px;
  background-color: #fff;
  overflow: hidden
}
.top image {
  margin-right: 10px;
}
.top .title {
  font-size: 18px;
  color: #333333;
}
.top .sub_title {
  font-size: 14px;
  color: #909090;
  margin-top: 15px;
}
.top .text text {
  display: block;
}
.show-title {
  margin: 15px 0
}
.center {
  padding: 30px 15px;
  padding-top: 15px;
  margin-bottom: 50px;
  background-color: #fff;
}
.center .item {
  position: relative;
  padding: 10px;
  padding-right: 30px;
  height: 55px;
  margin: 15px 0;
  justify-content: space-between;
  -webkit-justify-content: space-between;
  border: 1px solid #ddd;
}
.center .item .last {
  line-height: 50px;
  color: #666; 
}
.center .item.active {
  position: relative;
  border-color: #ffa201;
  background: #fffcee url(data:image/png;base64,) no-repeat 100% bottom;
  background-size: 36px;
}
.center .item .sub-title {
  margin-top: 10px;
  color: #909090;
  font-size: 14px;
}
.center .item .right {
  position: absolute;
  right: 30px;
  line-height: 0;
  top: 50%;
}
.center .item .right .sub {
  text-decoration: line-through;
  color: #909090;
  font-size: 15px;
  margin-right: 10px;
  font-weight: 700
}
.center .item .right .main {
  color: #ffa201;
  font-size: 18px;
  font-weight: 700
}
.bottom {
  position: fixed;
  width: 100%;
  bottom: 0;
  padding-left: 15px;
  height: 50px;
  line-height: 50px;
  background-color: #fff;
  font-size: 15px;
  box-sizing: border-box
}
.bottom .right {
  width: 30%;
  background-color: #382e2f;
  color: #fff;
  text-align: center
}
.bottom .left text {
  color: #ffa201;
}
.mask {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 1;
}